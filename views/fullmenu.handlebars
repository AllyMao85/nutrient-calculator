<div id='rawlist'>
<h2>Recipes List</h2>

<ul>
  {{#each recipes}}
    {{!-- {{#if anthenicated}}
      {{> recipes/recipes-block}}
    {{/if}} --}}
  {{/each}}
</ul>
</div>


<div id='add'>
<h2>ADD RECIPES</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">RECIPE NAME</label>
    <input type="text" id="ca" name="name">
  </div>


  <button type="submit">ADD RECIPE</button>
</form>
</div>